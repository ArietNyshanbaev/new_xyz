#: templates/postman/email_user.txt:3 templates/postman/email_visitor.txt:3
#, python-format
msgid "On %(date)s, you asked to send a message to the user '%(recipient)s'."
msgstr "于 %(date)s, 你向 '%(recipient)s' 发出了一条消息。"

